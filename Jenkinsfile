pipeline {
	agent any 
		stages {
			stage('Build') {
				steps {
					sh 'echo STAGE1: This is a build stage'
					}
					}
					stage('Test') {
						steps{
							sh 'echo STAGE2: This is test stage'
							}
					}
					stage('Deploy') {
							steps{
								sh 'echo STAGE3: This is deploy stage'
								}
							}
						}
					}
